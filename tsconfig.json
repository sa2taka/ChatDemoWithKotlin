{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es5",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "esnext"]
  },
  "include": ["src/main/js/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
